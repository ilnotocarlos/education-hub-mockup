{
  "name": "education-hub",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:web": "pnpm --filter @edu-hub/web run build",
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter @edu-hub/web run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.5.0"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.41.1"
    }
  },
  "packageManager": "pnpm@10.4.1"
}

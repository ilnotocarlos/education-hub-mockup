    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@edu-hub/types": "workspace:*",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/rate-limit": "^10.2.0",
    "@prisma/client": "^6.5.0",
    "@trpc/server": "^11.1.0",
    "bcryptjs": "^3.0.3",
    "fastify": "^5.3.0",
    "superjson": "^2.2.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@edu-hub/config": "workspace:*",
    "@types/node": "^20",
    "prisma": "^6.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5"
  }
}
